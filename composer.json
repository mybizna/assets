    "type": "library",
    "keywords": ["api","restful","laravel"],
    "version": "0.1.0",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Mybizna\\Assets\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mybizna",
            "homepage": "https://mybizna.com",
            "email": "info@mybizna.com"
        },
        {
            "name": "Dedan Irungu",
            "email": "dedanirungu@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mybizna\\Assets\\Providers\\MybiznaAssetsProvider"
            ]
        }
    },
    "scripts": {
        "copyHandlebars": [
            "echo 'GOOOD' && cp -r vendor/mybizna/assets/src/mybizna public"
